{
    "name": "iotronlab/laravel-page-view",
    "description": "Laravel Model Client View Counting Made Easy",
    "keywords": [
        "iotronlab",
        "laravel",
        "laravel-page-view"
    ],
    "homepage": "https://github.com/iotronlab/laravel-page-view",
    "license": "MIT",
    "authors": [
        {
            "name": "Krishanu",
            "email": "krishanu.info@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2|^1.14.0|^1.16.0",
        "illuminate/contracts": "^9.0|^10.0||^11.0",
        "nesbot/carbon": "^2.62.1}|^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0|^1.14",
        "nunomaduro/collision": "^6.0|^7.9|^8.1",
        "nunomaduro/larastan": "^2.0.1|2.9",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.21|^2.0|^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^1.1|^2.0|^2.3",
        "phpstan/extension-installer": "^1.1|^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.0|^1.1",
        "phpstan/phpstan-phpunit": "^1.0|^1.3",
        "spatie/laravel-ray": "^1.26|^1.35"
    },
    "autoload": {
        "psr-4": {
            "Iotronlab\\LaravelPageView\\": "src",
            "Iotronlab\\LaravelPageView\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iotronlab\\LaravelPageView\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iotronlab\\LaravelPageView\\LaravelPageViewServiceProvider"
            ],
            "aliases": {
                "LaravelPageView": "Iotronlab\\LaravelPageView\\Facades\\LaravelPageView"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
